{
    "name": "Word Lookup",
    "description": "Easily look up words on dictionaries.",
    "version": "1.2",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "contextMenus",
        "tabs"
    ],
    "icons": {
        "16": "/images/reading16.png",
        "32": "/images/reading32.png",
        "64": "/images/reading64.png",
        "128": "/images/reading128.png"
    }
}